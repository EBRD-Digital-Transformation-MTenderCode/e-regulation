{
  "schema": {
    "title": "Expenditure Item Creation",
    "type": "object",
    "properties": {
      "eiSubject": {
        "title": "Subject of Expenditure Item",
        "type": "object",
        "required": [
          "title"
        ],
        "properties": {
          "title": {
            "title": "Title (definition of type of object)",
            "type": "string",
            "ocds": "[tender][title]"
          },
          "classification": {
            "title": "Main CPV code",
            "type": "object",
            "required": [
              "scheme",
              "id",
              "description"
            ],
            "properties": {
              "scheme": {
                "title": "Classification scheme",
                "type": "string",
                "enum": [
                  "CPV"
                ],
                "ocds": "[tender][classification][scheme]"
              },
              "id": {
                "title": "CPV code",
                "type": "string",
                "enum": [
                  "03000000-1",
                  "03100000-2",
                  "03200000-3",
                  "03300000-2"
                ],
                "ocds": "[tender][classification][id]"
              },
              "description": {
                "title": "CPV name",
                "type": "string",
                "readOnly": true,
                "ocds": "[tender][classification][description]"
              }
            }
          },
          "description": {
            "title": "Short description",
            "type": "string",
            "ocds": "[tender][description]"
          },
          "period": {
            "title": "Period",
            "type": "object",
            "required": [
              "startDate",
              "endDate"
            ],
            "properties": {
              "startDate": {
                "title": "Start date",
                "type": "string",
                "format": "date-time",
                "ocds": "[planning][budget][period][startDate]"
              },
              "endDate": {
                "title": "End date",
                "type": "string",
                "format": "date-time",
                "ocds": "[planning][budget][period][endDate]"
              }
            }
          },
          "amount": {
            "title": "Amount",
            "type": "object",
            "required": [
              "currency"
            ],
            "properties": {
              "currency": {
                "title": "Currency",
                "type": "string",
                "enum": [
                  "EUR",
                  "UAH",
                  "MDL",
                  "RON",
                  "PLN",
                  "NOK"
                ],
                "ocds": "[planning][budget][amount][currency]"
              }
            }
          },
          "rationale": {
            "title": "Rationale",
            "type": "string",
            "ocds": "[planning][rationale]"
          }
        }
      },
      "eiBuyer": {
        "title": "Owner of Budget",
        "description": "Information about Entity who will operate budget from this Expenditure Item",
        "type": "object",
        "required": [
          "name"
        ],
        "properties": {
          "name": {
            "title": "Official Name",
            "type": "string",
            "ocds": "[buyer][name]"
          },
          "address": {
            "title": "Address",
            "type": "object",
            "properties": {
              "location": {
                "title": "Location",
                "type": "object",
                "required": [
                  "countryName",
                  "region",
                  "locality"
                ],
                "properties": {
                  "countryName": {
                    "title": "Country",
                    "type": "string",
                    "ocds": "[buyer][address][countryName]"
                  },
                  "region": {
                    "title": "Region",
                    "type": "string",
                    "ocds": "[buyer][address][region]"
                  },
                  "locality": {
                    "title": "Locality",
                    "type": "string",
                    "ocds": "[buyer][address][locality]"
                  }
                }
              },
              "postalAddress": {
                "title": "Postal Address",
                "type": "object",
                "required": [
                  "streetAddress"
                ],
                "properties": {
                  "streetAddress": {
                    "title": "Street Address",
                    "type": "string",
                    "ocds": "[buyer][address][streetAddress]"
                  },
                  "postalCode": {
                    "title": "Postal Code",
                    "type": "string",
                    "ocds": "[buyer][address][postalCode]"
                  }
                }
              }
            }
          },
          "identifier": {
            "title": "Main Identifier",
            "type": "object",
            "required": [
              "id",
              "scheme",
              "legalName",
              "uri"
            ],
            "properties": {
              "id": {
                "title": "Identifier",
                "type": "string",
                "ocds": "[buyer][identifier][id]"
              },
              "scheme": {
                "title": "Identification scheme",
                "type": "string",
                "ocds": "[buyer][identifier][scheme]"
              },
              "legalName": {
                "title": "Legal Name",
                "type": "string",
                "ocds": "[buyer][identifier][legalName]"
              },
              "uri": {
                "title": "URI",
                "type": "string",
                "ocds": "[buyer][identifier][uri]"
              }
            }
          },
          "additionalIdentifiers": {
            "title": "Additional Identifiers",
            "type": "array",
            "items": {
              "type": "object",
              "required": [
                "id",
                "scheme",
                "legalName",
                "uri"
              ],
              "properties": {
                "id": {
                  "title": "Additional identifier ID",
                  "type": "string",
                  "ocds": "[buyer][additionalIdentifiers][n][id]"
                },
                "scheme": {
                  "title": "Additional identifier scheme",
                  "type": "string",
                  "ocds": "[buyer][additionalIdentifiers][n][scheme]"
                },
                "legalName": {
                  "title": "Additional identifier legal name",
                  "type": "string",
                  "ocds": "[buyer][additionalIdentifiers][n][legalName]"
                },
                "uri": {
                  "title": "Additional identifier URI",
                  "type": "string",
                  "ocds": "[buyer][additionalIdentifiers][n][uri]"
                }
              }
            }
          },
          "details": {
            "title": "Details",
            "type": "object",
            "required": [
              "typeOfBuyer",
              "mainGeneralActivity",
              "mainSectoralActivity",
              "scale"
            ],
            "properties": {
              "typeOfBuyer": {
                "title": "Type of buyer",
                "type": "string",
                "enum": [
                  "BODY_PUBLIC",
                  "EU_INSTITUTION",
                  "MINISTRY",
                  "NATIONAL_AGENCY",
                  "REGIONAL_AGENCY",
                  "REGIONAL_AUTHORITY"
                ],
                "ocds": "[buyer][details][typeOfBuyer]"
              },
              "mainGeneralActivity": {
                "title": "Main general activity",
                "type": "string",
                "enum": [
                  "DEFENCE",
                  "ECONOMIC_AND_FINANCIAL_AFFAIRS",
                  "EDUCATION",
                  "ENVIRONMENT",
                  "GENERAL_PUBLIC_SERVICES",
                  "HEALTH",
                  "HOUSING_AND_COMMUNITY_AMENITIES",
                  "PUBLIC_ORDER_AND_SAFETY",
                  "RECREATION_CULTURE_AND_RELIGION",
                  "SOCIAL_PROTECTION"
                ],
                "ocds": "[buyer][details][mainGeneralActivity]"
              },
              "mainSectoralActivity": {
                "title": "Main sectoral activity",
                "type": "string",
                "enum": [
                  "AIRPORT_RELATED_ACTIVITIES",
                  "ELECTRICITY",
                  "EXPLORATION_EXTRACTION_COAL_OTHER_SOLID_FUEL",
                  "EXPLORATION_EXTRACTION_GAS_OIL",
                  "PORT_RELATED_ACTIVITIES",
                  "POSTAL_SERVICES",
                  "PRODUCTION_TRANSPORT_DISTRIBUTION_GAS_HEAT",
                  "RAILWAY_SERVICES",
                  "URBAN_RAILWAY_TRAMWAY_TROLLEYBUS_BUS_SERVICES",
                  "WATER"
                ],
                "ocds": "[buyer][details][mainSectoralActivity]"
              },
              "isACentralPurchasingBody": {
                "title": "Is a central purchasing body",
                "type": "boolean",
                "readOnly": true,
                "default": false,
                "ocds": "[buyer][details][isACentralPurchasingBody]"
              },
              "scale": {
                "title": "Scale",
                "type": "string",
                "enum": [
                  "micro",
                  "sme",
                  "large"
                ],
                "ocds": "[buyer][details][scale]"
              }
            }
          },
          "buyerProfile": {
            "title": "Buyer Profile",
            "type": "string",
            "ocds": "[buyer][buyerProfile]"
          },
          "contactPoint": {
            "title": "Contact point",
            "type": "object",
            "properties": {
              "person": {
                "title": "Contact person",
                "type": "object",
                "required": [
                  "name",
                  "url"
                ],
                "properties": {
                  "name": {
                    "title": "Name",
                    "type": "string",
                    "ocds": "[buyer][contactPoint][name]"
                  },
                  "url": {
                    "title": "URL",
                    "type": "string",
                    "ocds": "[buyer][contactPoint][url]"
                  }
                }
              },
              "contacts": {
                "title": "Contact point",
                "type": "object",
                "required": [
                  "telephone",
                  "email"
                ],
                "properties": {
                  "telephone": {
                    "title": "Telephone",
                    "type": "string",
                    "ocds": "[buyer][contactPoint][telephone]"
                  },
                  "email": {
                    "title": "E-mail",
                    "type": "string",
                    "ocds": "[buyer][contactPoint][email]"
                  },
                  "faxNumber": {
                    "title": "Fax number",
                    "type": "string",
                    "ocds": "[buyer][contactPoint][faxNumber]"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "data": {
    "eiSubject": {
      "title": "gdsjkhhgfoiur"
    }
  }
}
